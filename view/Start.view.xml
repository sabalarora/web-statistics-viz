	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Heatmap" subheader="Daily Usage" press="onTileButtonClicked">
		<TileContent>
			    <core:Icon
					src="sap-icon://heatmap-chart" 
					size="3.5em"
					color="#93b7d6"
				/>
			<mvc:XMLView viewName="com.dla.webstat.view.statistics.Heatmap"/>
		</TileContent>
	</GenericTile>

	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="2 Line Graph" subheader="Expenses" press="onTileButtonClicked">
		<TileContent>
			<core:Icon
					src="sap-icon://line-charts" 
					size="3.5em"
					color="#93b7d6"
				/>
				<mvc:XMLView viewName="com.dla.webstat.view.statistics.Line"/>
		</TileContent>
	</GenericTile>

	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Total Usage" subheader="Expenses" id="barChart" press="onTileButtonClicked">
		<TileContent>
			<core:Icon
					src="sap-icon://horizontal-bar-chart" 
					size="3.5em"
					color="#93b7d6"
				/>
				 <mvc:XMLView viewName="com.dla.webstat.view.statistics.Bar"/> 
		</TileContent>
	</GenericTile>


