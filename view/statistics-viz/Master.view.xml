<mvc:View controllerName="com.dla.webstat.controller.statistics-viz.Master" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
    xmlns:semantic="sap.m.semantic">
	<semantic:MasterPage
	showNavButton="true">
		<semantic:subHeader>
		</semantic:subHeader>
		<semantic:content>
			<!-- <Toolbar class="measureToolBar">
				<Label text="Aggregation Level" class="toolBarTextHeader"></Label>
			</Toolbar>
			<Select id="selectPopover" width="100%" name="test" class="selectPop" items="{
					path: '/aggregations'
				}">
				<items>
					<core:Item text="{name}" key="Default" />
				</items>
			</Select> -->

			<List id="MeasureList" mode="MultiSelect" selectionChange="onSelectionChange" includeItemInSelection="true" items="{
					path: '/dataset/measures'
				}">
				<headerToolbar>
					<Toolbar class="measureToolBar">
						<Label text="Measures" class="toolBarTextHeader"></Label>
					</Toolbar>
				</headerToolbar>
				<items>
					<StandardListItem title="{name}">
					</StandardListItem>
				</items>
			</List>
			<List id="idoApplist" mode="MultiSelect" includeItemInSelection="true" items="{
					path: '/'
				}">
				<headerToolbar>
					<Toolbar class="measureToolBar">
						<Label text="Applications" class="toolBarTextHeader"></Label>
					</Toolbar>
				</headerToolbar>
				<items>
					<StandardListItem type="{= ${device>/system/phone} ? 'Active' : 'Inactive'}" press="onSelectionChange" title="{object}">
					</StandardListItem>
				</items>
			</List>
		</semantic:content>
	</semantic:MasterPage>
</mvc:View>